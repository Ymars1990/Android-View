                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <com.mars.marsview.view.MyRoundImageView
                        android:id="@+id/myroundimageview02"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:src="@drawable/titleicon"
                        mars:myroundimageview_circle="true"
                        mars:myroundimageview_radius="20dp" >
                    </com.mars.marsview.view.MyRoundImageView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <com.mars.marsview.view.MyRoundImageView
                        android:id="@+id/myroundimageview05"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:src="@drawable/titleicon"
                        mars:myroundimageview_circle="false"
                        mars:myroundimageview_radius="20dp" >
                    </com.mars.marsview.view.MyRoundImageView>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1.0"
                android:gravity="center"
                android:orientation="horizontal"
                android:padding="5dp" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <com.mars.marsview.view.MyRoundImageView
                        android:id="@+id/myroundimageview03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
