                        android:id="@+id/myroundimageview09"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:src="@drawable/titleicon"
                        mars:myroundimageview_circle="true"
                        mars:myroundimageview_radius="20dp" >
                    </com.mars.marsview.view.MyRoundImageView>
                </LinearLayout>

